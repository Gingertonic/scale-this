
  <form class="new_practise" id="new_practise" action="/practises">
    {{#with scale}}
      <input type="hidden" value="{{id}}" name="practise[scale_id]" id="practise_scale_id">
    {{/with}}
    {{#with musician}}
      <input type="hidden" value="{{id}}" name="practise[musician_id]" id="practise_musician_id">
    {{/with}}

      <input type="submit" id="submit_practise" name="commit" value="Practised!">
  </form>
